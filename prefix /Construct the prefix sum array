public class PrefixSumArray {
    public static void main(String[] args) {
        int[] arr = {5, -2, 3, 1, 2};
        int n = arr.length;

        int[] prefix = new int[n];
        prefix[0] = arr[0]; 

        for (int i = 1; i < n; i++) {
            prefix[i] = prefix[i - 1] + arr[i];
        }

        // print prefix sum array
        for (int i = 0; i < n; i++) {
            System.out.print(prefix[i] + " ");
        }
    }
}
