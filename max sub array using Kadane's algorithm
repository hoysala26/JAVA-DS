public class Pair {
    public static void main(String[] args) {
        int[]arr={1,4,-3,-5,3,-4};
        int max = 0;
        int currentsum= 0;
        for(int i=0;i<arr.length;i++){
            
                currentsum=Math.max(arr[i], currentsum+arr[i]);
                max=Math.max(max, currentsum);
        } 
        System.out.println(max);
    }
}   
